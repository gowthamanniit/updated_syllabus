<form oninput="res.value=t1.value*500 + t2.value*200 ">
		
		500 X <input type="text" value="0" name="t1"><br><br>
		200 X <input type="text" value="0"name="t2"><br><br><br><br>

		Total Amount:<output name="res"></output>

	</form>
